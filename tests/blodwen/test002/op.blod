(+) : Int -> Int -> Int
(+) = prim__add_Int

(-) : Int -> Int -> Int
(-) = prim__sub_Int

(*) : Int -> Int -> Int
(*) = prim__mul_Int

(/) : Int -> Int -> Int
(/) = prim__div_Int

(-:) : Int -> Int
(-:) x = prim__sub_Int 0 x

